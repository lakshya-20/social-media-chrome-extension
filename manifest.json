{
    "manifest_version": 3,
    "name": "Social Media Extension",
    "description": "Instant social media profiles",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }